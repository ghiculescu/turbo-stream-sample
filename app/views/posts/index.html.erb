<p style="color: green"><%= notice %></p>

<h1>Posts</h1>

<%= turbo_frame_tag "posts" do %>
  <div id="posts">
    <% @posts.each do |post| %>
      <%= render post %>
    <% end %>
  </div>
<% end %>

<%= link_to "New post", new_post_path, data: { turbo_frame: "new_post" } %>

<%= turbo_frame_tag "new_post" %>
